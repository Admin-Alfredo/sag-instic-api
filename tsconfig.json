{
  "compilerOptions": {
      "target": "es6",
      "module": "NodeNext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "NodeNext",
      "sourceMap": true,
      "outDir": "./dist",
      "rootDir": "./src",
  },
  
  "include": [
      "src/**/*"
, "prisma/seeds"  ],
}